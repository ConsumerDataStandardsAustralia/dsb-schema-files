 {
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://consumerdatastandards.org.au/v1.0.0/accounts",
	"type": "object",
	"required": [
		"errors"
	],
	"properties": {
        "errors": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [
				    "code",
				    "title",
				    "detail"
				],
				"properties": {
				    "code": {
						"type": "string",
						"pattern": "^\\d{4,4}$"
					},
				    "title": {
                        "type" : "string"
					},
				    "detail": {
                        "type" : "string"
					},
				    "meta": {
 					}
				}
			}
		}
	}
}
